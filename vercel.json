{
  "version": 2,
  "builds": [
    {
      "src": "apps/web/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ],
  "env": {
    "ROLLUP_USE_NATIVE": "false",
    "CI": "true"
  },
  "buildCommand": "cd apps/web && npm ci && npm run build",
  "devCommand": "cd apps/web && npm run dev",
  "installCommand": "cd apps/web && npm ci",
  
}
