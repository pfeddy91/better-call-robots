{
  "name": "@better-call-robots/gemini-live-api",
  "version": "1.0.0",
  "description": "Real-time voice AI service using Google Gemini Live API with Twilio telephony integration",
  "main": "dist/index.js",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
    "dev": "PORT=8081 nodemon",
    "build": "tsc && tsconfig-paths -b . -p tsconfig.json",
    "serve": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:api": "./test-gemini.sh",
    "test:interface": "open test-interface.html",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "gemini",
    "twilio",
    "voice-ai",
    "websocket",
    "real-time",
    "conversational-ai"
  ],
  "author": "Better Call Robots",
  "license": "MIT",
  "dependencies": {
    "@google/genai": "^1.12.0",
    "@google/generative-ai": "^0.8.0",
    "@types/ws": "^8.18.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "multer": "^1.4.5-lts.1",
    "twilio": "^4.20.1",
    "uuid": "^9.0.1",
    "wavefile": "^11.0.0",
    "winston": "^3.11.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
